<div id="<%= dom_id lesson %>">
  <p>
    <strong>Calendar:</strong>
    <%= lesson.calendar_id %>
  </p>

  <p>
    <strong>Teacher:</strong>
    <%= lesson.teacher_id %>
  </p>

  <p>
    <strong>Student:</strong>
    <%= lesson.student_id %>
  </p>


  <p>
    <strong>Started at:</strong>
    <%= lesson.starts_at %>
  </p>

  <p>
    <strong>ends_at:</strong>
    <%= lesson.ends_at %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= lesson.status %>
  </p>

  <p>
    <strong>Lesson notes:</strong>
    <%= lesson.lesson_notes %>
  </p>

</div>
