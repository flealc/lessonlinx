<%= turbo_stream.update "students-list" do %>
  <%= render partial: "students/students_list" %>
<% end %>

<%= turbo_stream.update "student-details" do %>
  
  <script>
      document.getElementById("student-details").src = "<%= student_path(current_user.students.first) %>"
      history.pushState({ turbo: true }, "<%= student_path(@student) %>")
  </script>

<% end %>
